{
    "project": "ndbf",
    "plugins": [
    {
      "id": "asset-library",
      "title": "Assets",
      "environments": [ "edit" ],
      "url": "https://experience.adobe.com/solutions/CQ-helix-assets-addon/static-assets/resources/asset-selector.html",
      "isPalette": true,
      "includePaths": [
        "**.docx**"
      ],
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:800px; height: calc(100vh - 60px)",
      "copyMode": [
        {
          "mimeType": "image/*",
          "value": "reference"
        }
      ]
    }
    ]
  }
